<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://editorCaption"
        class="com.gitlab.daring.sandbox.cuba.web.ruletype.RuleTypeEdit"
        datasource="ruleTypeDs"
        focusComponent="fieldGroup"
        messagesPack="com.gitlab.daring.sandbox.cuba.web.ruletype">
    <dsContext>
        <datasource id="ruleTypeDs"
                    class="com.gitlab.daring.sandbox.cuba.entity.RuleType"
                    view="_local"/>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout expand="windowActions"
            spacing="true">
        <fieldGroup id="fieldGroup"
                    datasource="ruleTypeDs">
            <column width="250px">
                <field property="code"/>
            </column>
        </fieldGroup>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>
